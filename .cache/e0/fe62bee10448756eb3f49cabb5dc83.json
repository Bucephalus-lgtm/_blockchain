dData;\r\n\r\n        if (this.state.displayTransaction) {\r\n            return (\r\n                <div>\r\n                    <div>\r\n                        {\r\n                            data.map(transaction => (\r\n                                <div key={transaction.id}> <hr />\r\n                                    <Transaction key={transaction.id} transaction={transaction} />\r\n                                </div>\r\n                            ))\r\n                        }\r\n                    </div>\r\n                    <Button\r\n                        bsStyle=\"info\"\r\n                        bsSize=\"small\"\r\n                        onClick={this.toggleTransaction}\r\n\r\n                    >\r\n                        Show Less\r\n                    </Button>\r\n                </div>\r\n            )\r\n        }\r\n\r\n        return (\r\n            <div>\r\n                <div>Data: {dataDisplay}</div>\r\n                <Button\r\n                    bsSize=\"small\"\r\n                    bsStyle=\"info\"\r\n                    onClick={this.toggleTransaction}\r\n                >Show more</Button>\r\n            </div>\r\n        )\r\n    }\r\n\r\n    render() {\r\n\r\n        const { timestamp, hash, lastHash, data } = this.props.block;\r\n\r\n        const hashDisplay = `${hash.substring(0, 15)}.....`;\r\n\r\n        return (\r\n            <div className=\"block\">\r\n                <div>Hash: {hashDisplay}</div>\r\n                <div>Timestamp: {new Date(timestamp).toLocaleDateString()}</div>\r\n                <div>LastHash: {lastHash}</div>\r\n\r\n                {this.displayTransaction}\r\n            </div>\r\n        )\r\n    }\r\n}\r\n\r\nexport default Block;"},"lineCount":null}},"error":null,"hash":"ae6b97e3a1766b58622d470f72890414","cacheData":{"env":{}}}